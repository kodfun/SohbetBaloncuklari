{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@3.1.8",
      "destination": "wwwroot/js/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/js/jquery/",
      "files": [
        "dist/jquery.min.js"
      ]
    },
    {
      "library": "jquery-ui-dist@1.12.1",
      "destination": "wwwroot/js/jquery-ui-dist/",
      "files": [
        "jquery-ui.min.css",
        "jquery-ui.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jqueryui-touch-punch@0.2.3",
      "destination": "wwwroot/js/jqueryui-touch-punch/",
      "files": [
        "jquery.ui.touch-punch.min.js"
      ]
    }
  ]
}